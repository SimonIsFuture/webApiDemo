<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
    <script src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.8.0.js">
    </script>
</head>
<body>
    <p>Input the url:</p>
    <input type="text" id="input" placeholder="input query url">
    <button id="btn">Ok</button>
    <button id="btn1">OK</button>
    <p id="res">The response is</p>>



</body>
<script>
    var input=$("#input");
    var btn=$("#btn");
    var res = $("#res");
    var btn1 = $("#btn1");
    btn1.click(
        function () {
            window.location.href = "./testPage.cshtml";
        }
        )

    btn.click(
            function () {
                var url=input.val();
                $.ajax(
                        {
                            type:"GET",
                            contentType:"application/json",
                            url:url,
                            dataType:'json',
                            success:function(res){
                                alert(res);
                            }
                        }
                )
            }
    );
</script>
</html>